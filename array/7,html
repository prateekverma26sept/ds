<pre><code>
    #include&lt;stdio.h&gt;
    #include&lt;conio.h&gt;
    #include&lt;dos.h&gt;
    void main(){
        int arr[5], i;
        clrscr();
        for (i = 0; i &lt; 5; i++)
        {
        printf("Enter Value of position %d\n", i+1);
        scanf("%d", &arr[i]);
        arr[i] = arr[i] * arr[i];
        }
        printf("Squared Array\n");
        for (i = 0; i &lt; 5; i++)
        {
        printf("%d\n", arr[i]);
        }
        delay(5000);
    }
        </code>
